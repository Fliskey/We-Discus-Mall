s.$refs['form'].validate((valid) => {\n          if (valid) {\n            console.log(valid)\n            // 路由注册\n            this.$router.push({\n              path: '/admin/user'\n            })\n          }\n        })\n      },\n      change (type) {\n        this.$emit('changeType', type)\n      }\n    }\n  }\n</script>\n"]}]}