A;AAKA;AAPA;AApCA,CAAA","sourcesContent":["<template>\n  <a-row class='cen-box'>\n    <a-col :span='12'>\n      <h2>修改个人信息</h2>\n      <a-form-model ref='form' :model=\"form\" :rules='rules'>\n        <a-form-model-item prop='psw'>\n          <a-input v-model='form.psw' block type='学号' disabled placeholder='请输入学号'>\n            <a-icon slot=\"prefix\" type=\"user\" style=\"color:rgba(0,0,0,.25)\" />\n          </a-input>\n        </a-form-model-item>\n        <a-form-model-item  prop='phone'>\n          <a-input v-model='form.psw1' block type='password' placeholder='请输入手机号'>\n            <a-icon slot=\"prefix\" type=\"phone\" style=\"color:rgba(0,0,0,.25)\" />\n          </a-input>\n        </a-form-model-item>\n        <a-form-model-item prop='email'>\n          <a-input v-model='form.psw2' block type='password' placeholder='请输入邮箱'>\n            <a-icon slot=\"prefix\" type=\"mail\" style=\"color:rgba(0,0,0,.25)\" />\n          </a-input>\n        </a-form-model-item>\n        <a-form-model-item prop='verifyCode'>\n        </a-form-model-item>\n        <a-form-model-item>\n          <a-button type=\"primary\" block @click='submit'>确认修改</a-button>\n        </a-form-model-item>\n      </a-form-model>\n    </a-col>\n  </a-row>\n</template>\n\n<script>\n  export default {\n    data () {\n      return {\n        wrapperCol: { span: 20 },\n        form: {\n          name: '',\n          psw: '',\n          psw1: '',\n          psw2: ''\n        },\n        rules: {\n          psw: {\n            required: true,\n            message: '请输入原登录密码',\n            trigger: 'blur'\n          },\n          psw1: {\n            required: true,\n            message: '请输入新密码',\n            trigger: 'blur'\n          },\n          psw2: {\n            required: true,\n            message: '请再次输入新密码',\n            trigger: 'blur'\n          }\n        }\n      }\n    },\n    created() {\n      let id = this.$route.query.id\n      let _this = this\n      axios.get('http://localhost:8181/umUser/find/'+id).then(function (response) {\n        _this.UmUser = response.data\n      })\n    },\n    methods: {\n      submit () {\n        this.$refs['form'].validate((valid) => {\n          if (valid) {\n            console.log(valid)\n          }\n        })\n      }\n    }\n  }\n</script>\n<style lang='less'>\n  @import \"~@/style/center.less\";\n</style>\n"],"sourceRoot":"src/views/center"}]}