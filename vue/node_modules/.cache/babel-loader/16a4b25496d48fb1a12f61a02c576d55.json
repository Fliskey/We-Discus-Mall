   this.$refs['form'].validate((valid) => {\n          if (valid) {\n            console.log(valid)\n            // 路由注册\n            this.$router.push({\n              path: '/admin/user'\n            })\n          }\n        })\n      },\n      changeType () {\n        this.type = this.type ==='login' ? 'register' : 'login'\n      }\n    }\n  }\n</script>\n\n<style lang='less'>\n  @import \"~@/style/login.less\";\n</style>\n"],"sourceRoot":"src/page/login"}]}