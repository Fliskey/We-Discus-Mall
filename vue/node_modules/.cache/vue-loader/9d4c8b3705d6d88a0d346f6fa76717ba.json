$router.push({\n          path: '/admin/goods/detail'\n        })\n      }\n    }\n  }\n</script>\n\n<style lang=\"less\" scoped>\n  .extra{\n    width: 272px;\n    height: 1px;\n  }\n  .content {\n    .detail {\n      line-height: 22px;\n      max-width: 720px;\n      flex-wrap:nowrap;\n      > img{\n        flex: 1;\n        margin-right: 15px;\n      }\n      > p{\n        max-height:110px;\n        overflow: hidden;\n        text-overflow: ellipsis;\n        cursor:pointer;\n        display: -webkit-box;\n        -webkit-line-clamp: 5;\n        -webkit-box-orient: vertical;\n      }\n    }\n    .author {\n      color: #999;\n      margin-top: 16px;\n      line-height: 22px;\n      & > :global(.ant-avatar) {\n          vertical-align: top;\n          margin-right: 8px;\n          width: 20px;\n          height: 20px;\n          position: relative;\n          top: 1px;\n        }\n      & > a{\n        padding:0 10px;\n      }\n      & > em {\n          color: #666;\n          font-style: normal;\n        }\n    }\n  }\n</style>\n"]}]}